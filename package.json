{
  "name": "busy",
  "main": "./lib/index",
  "version": "0.2.0",
  "description": "A generic display and progress for things that take time",
  "keywords": [
    "progress",
    "status-bar",
    "pending"
  ],
  "repository": "https://github.com/noseglid/atom-busy",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "busy": {
      "versions": {
        "1.0.0": "provideRegistry"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.9.0",
    "eslint-config-atom-build": "^3.0.0"
  },
  "configSchema": {
    "taskBacklog": {
      "type": "integer",
      "default": 5,
      "minimum": 1,
      "description": "The number of task to keep in the history"
    }
  }
}
